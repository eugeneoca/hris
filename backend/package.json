{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.ts",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "production": "node dist/index.js --env-file=production.env",
    "dev": "nodemon dist/index.js",
    "watch": "tsc-watch -p ./tsconfig.json --onSuccess \"npm run dev\"",
    "migrate": "npx prisma migrate dev",
    "migrate:generate": "npx prisma migrate dev --create-only",
    "migrate:reset": "npx prisma migrate reset -f && npx prisma generate",
    "build": "tsc --build",
    "init": "npm install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-list-routes": "^1.2.1",
    "express-resource": "^1.0.0",
    "express-validator": "^7.1.0",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.10.0",
    "prisma": "^5.14.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@prisma/client": "^5.14.0",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.13.0",
    "@types/validator": "^13.11.10",
    "body-parser": "^1.20.2",
    "nodemon": "^3.1.2",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.4.5"
  },
  "description": ""
}
